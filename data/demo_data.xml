<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Sample Facilities -->
    <record id="facility_tennis_court" model="sports.facility">
            <field name="name">Tennis Court A</field>
            <field name="facility_type">court</field>
            <field name="description">Professional tennis court with clay surface, suitable for singles and doubles matches. Includes night lighting and seating area.</field>
            <field name="capacity">4</field>
            <field name="hourly_rate">25.00</field>
            <field name="status">available</field>
            <field name="location">North Wing, Building A</field>
            <field name="operating_hours_start">7.0</field>
            <field name="operating_hours_end">22.0</field>
        </record>

        <record id="facility_gym" model="sports.facility">
            <field name="name">Main Fitness Gym</field>
            <field name="facility_type">gym</field>
            <field name="description">Fully equipped gym with cardio machines, weight training equipment, and functional training area. Air-conditioned with modern amenities.</field>
            <field name="capacity">30</field>
            <field name="hourly_rate">15.00</field>
            <field name="status">available</field>
            <field name="location">Ground Floor, Building B</field>
            <field name="operating_hours_start">6.0</field>
            <field name="operating_hours_end">23.0</field>
        </record>

        <record id="facility_swimming_pool" model="sports.facility">
            <field name="name">Olympic Swimming Pool</field>
            <field name="facility_type">pool</field>
            <field name="description">50-meter Olympic-size swimming pool with 8 lanes. Heated water, diving boards, and lifeguard services included.</field>
            <field name="capacity">50</field>
            <field name="hourly_rate">30.00</field>
            <field name="status">available</field>
            <field name="location">Aquatic Center</field>
            <field name="operating_hours_start">6.0</field>
            <field name="operating_hours_end">21.0</field>
        </record>

        <record id="facility_soccer_field" model="sports.facility">
            <field name="name">Outdoor Soccer Field</field>
            <field name="facility_type">field</field>
            <field name="description">Full-size regulation soccer field with natural grass. Includes goals, corner flags, and spectator bleachers.</field>
            <field name="capacity">22</field>
            <field name="hourly_rate">40.00</field>
            <field name="status">available</field>
            <field name="location">Outdoor Sports Complex</field>
            <field name="operating_hours_start">8.0</field>
            <field name="operating_hours_end">20.0</field>
        </record>

        <record id="facility_basketball_court" model="sports.facility">
            <field name="name">Indoor Basketball Court</field>
            <field name="facility_type">court</field>
            <field name="description">Indoor basketball court with maple hardwood flooring, professional hoops, and scoreboard. Climate controlled environment.</field>
            <field name="capacity">10</field>
            <field name="hourly_rate">20.00</field>
            <field name="status">available</field>
            <field name="location">South Wing, Building C</field>
            <field name="operating_hours_start">7.0</field>
            <field name="operating_hours_end">22.0</field>
        </record>

        <!-- Sample Equipment -->
        <record id="equipment_tennis_rackets" model="sports.equipment">
            <field name="name">Professional Tennis Rackets</field>
            <field name="equipment_type">racket</field>
            <field name="total_quantity">10</field>
            <field name="quantity_available">10</field>
            <field name="rental_rate">5.00</field>
            <field name="condition">excellent</field>
        </record>

        <record id="equipment_basketballs" model="sports.equipment">
            <field name="name">Official Size Basketballs</field>
            <field name="equipment_type">ball</field>
            <field name="total_quantity">15</field>
            <field name="quantity_available">15</field>
            <field name="rental_rate">3.00</field>
            <field name="condition">good</field>
        </record>

        <record id="equipment_yoga_mats" model="sports.equipment">
            <field name="name">Premium Yoga Mats</field>
            <field name="equipment_type">mat</field>
            <field name="total_quantity">20</field>
            <field name="quantity_available">20</field>
