        <!-- Sample Memberships -->
        <record id="membership_john_basic" model="sports.membership">
            <field name="member_id" ref="customer_john_doe"/>
            <field name="membership_type">basic</field>
            <field name="start_date" eval="(datetime.now() - timedelta(days=30)).strftime('%Y-%m-%d')"/>
            <field name="end_date" eval="(datetime.now() + timedelta(days=335)).strftime('%Y-%m-%d')"/>
            <field name="discount_percentage">10.00</field>
            <field name="status">active</field>
            <field name="payment_status">paid</field>
            <field name="membership_fee">99.00</field>
            <field name="notes">Basic membership with 10% discount on all bookings</field>
        </record>

        <record id="membership_jane_premium" model="sports.membership">
            <field name="member_id" ref="customer_jane_smith"/>
            <field name="membership_type">premium</field>
            <field name="start_date" eval="(datetime.now() - timedelta(days=60)).strftime('%Y-%m-%d')"/>
            <field name="end_date" eval="(datetime.now() + timedelta(days=305)).strftime('%Y-%m-%d')"/>
            <field name="discount_percentage">20.00</field>
            <field name="status">active</field>
            <field name="payment_status">paid</field>
            <field name="membership_fee">199.00</field>
            <field name="notes">Premium membership with 20% discount on all bookings</field>
        </record>

        <record id="membership_mike_vip" model="sports.membership">
            <field name="member_id" ref="customer_mike_johnson"/>
            <field name="membership_type">vip</field>
            <field name="start_date" eval="(datetime.now() - timedelta(days=90)).strftime('%Y-%m-%d')"/>
            <field name="end_date" eval="(datetime.now() + timedelta(days=275)).strftime('%Y-%m-%d')"/>
            <field name="discount_percentage">30.00</field>
            <field name="status">active</field>
            <field name="payment_status">paid</field>
            <field name="membership_fee">399.00</field>
            <field name="notes">VIP membership with 30% discount on all bookings and priority access</field>
        </record>

        <!-- Sample Bookings -->
        <record id="booking_tennis_confirmed" model="sports.booking">
            <field name="booking_reference">BOOK/2025/0001</field>
            <field name="facility_id" ref="facility_tennis_court"/>
            <field name="customer_id" ref="customer_john_doe"/>
            <field name="start_datetime" eval="(datetime.now() + timedelta(days=2, hours=10)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_datetime" eval="(datetime.now() + timedelta(days=2, hours=12)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="status">confirmed</field>
            <field name="notes">Tennis practice session with coach</field>
        </record>

        <record id="booking_gym_draft" model="sports.booking">
            <field name="booking_reference">BOOK/2025/0002</field>
            <field name="facility_id" ref="facility_gym"/>
            <field name="customer_id" ref="customer_jane_smith"/>
            <field name="start_datetime" eval="(datetime.now() + timedelta(days=3, hours=8)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_datetime" eval="(datetime.now() + timedelta(days=3, hours=9)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="status">draft</field>
            <field name="notes">Morning workout session</field>
        </record>

        <record id="booking_pool_completed" model="sports.booking">
            <field name="booking_reference">BOOK/2025/0003</field>
            <field name="facility_id" ref="facility_swimming_pool"/>
            <field name="customer_id" ref="customer_mike_johnson"/>
            <field name="start_datetime" eval="(datetime.now() - timedelta(days=1, hours=14)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_datetime" eval="(datetime.now() - timedelta(days=1, hours=15)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="status">completed</field>
            <field name="notes">Swimming training session completed</field>
        </record>

        <record id="booking_soccer_confirmed" model="sports.booking">
            <field name="booking_reference">BOOK/2025/0004</field>
            <field name="facility_id" ref="facility_soccer_field"/>
            <field name="customer_id" ref="customer_sarah_williams"/>
            <field name="start_datetime" eval="(datetime.now() + timedelta(days=5, hours=15)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_datetime" eval="(datetime.now() + timedelta(days=5, hours=17)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="status">confirmed</field>
            <field name="notes">Team practice match</field>
        </record>

        <record id="booking_basketball_cancelled" model="sports.booking">
            <field name="booking_reference">BOOK/2025/0005</field>
            <field name="facility_id" ref="facility_basketball_court"/>
            <field name="customer_id" ref="customer_robert_brown"/>
            <field name="start_datetime" eval="(datetime.now() + timedelta(days=1, hours=18)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="end_datetime" eval="(datetime.now() + timedelta(days=1, hours=20)).strftime('%Y-%m-%d %H:%M:%S')"/>
            <field name="status">cancelled</field>
            <field name="notes">Cancelled due to scheduling conflict</field>
        </record>

        <!-- Link equipment to bookings -->
        <record id="booking_tennis_equipment_rel" model="sports.booking">
            <field name="id" ref="booking_tennis_confirmed"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_tennis_rackets')])]"/>
        </record>

        <record id="booking_gym_equipment_rel" model="sports.booking">
            <field name="id" ref="booking_gym_draft"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_dumbbells'), ref('equipment_yoga_mats')])]"/>
        </record>

        <record id="booking_soccer_equipment_rel" model="sports.booking">
            <field name="id" ref="booking_soccer_confirmed"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_soccer_balls'), ref('equipment_volleyball_nets')])]"/>
        </record>

        <!-- Link equipment to facilities -->
        <record id="facility_tennis_equipment" model="sports.facility">
            <field name="id" ref="facility_tennis_court"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_tennis_rackets')])]"/>
        </record>

        <record id="facility_gym_equipment" model="sports.facility">
            <field name="id" ref="facility_gym"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_dumbbells'), ref('equipment_yoga_mats'), ref('equipment_resistance_bands'), ref('equipment_jump_ropes'), ref('equipment_weight_benches')])]"/>
        </record>

        <record id="facility_pool_equipment" model="sports.facility">
            <field name="id" ref="facility_swimming_pool"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_pool_floats')])]"/>
        </record>

        <record id="facility_soccer_equipment" model="sports.facility">
            <field name="id" ref="facility_soccer_field"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_soccer_balls'), ref('equipment_volleyball_nets')])]"/>
        </record>

        <record id="facility_basketball_equipment" model="sports.facility">
            <field name="id" ref="facility_basketball_court"/>
            <field name="equipment_ids" eval="[(6, 0, [ref('equipment_basketballs')])]"/>
        </record>

</odoo>
