        <record id="equipment_jump_ropes" model="sports.equipment">
            <field name="name">Speed Jump Ropes</field>
            <field name="equipment_type">weights</field>
            <field name="total_quantity">10</field>
            <field name="quantity_available">10</field>
            <field name="rental_rate">1.00</field>
            <field name="condition">good</field>
        </record>

        <record id="equipment_weight_benches" model="sports.equipment">
            <field name="name">Adjustable Weight Benches</field>
            <field name="equipment_type">weights</field>
            <field name="total_quantity">3</field>
            <field name="quantity_available">3</field>
            <field name="rental_rate">5.00</field>
            <field name="condition">excellent</field>
        </record>

